@model  Lumos.Web.MessageBox
@{
    ViewBag.Title = "信息提示";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";

}

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $.lumos.messageBox({ type: "@Model.Type", isPopup: @(Model.IsPopup==true? "true":"false"), title: "@Model.Title", content: "@Model.Content", errorStackTrace: "@Html.Raw(Model.ErrorStackTrace)" })
        });
    </script>
}
